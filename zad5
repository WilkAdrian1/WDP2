#include <iostream>
#include <vector>

using namespace std;

void wypisz(vector<int>tab)
{
    for (int i = 0; i < tab.size(); i++)
    {
        cout << tab[i] << ".";
    }
    cout << endl;
}

int max(vector<int>tab)
{
    int wynik = tab[0];
    for (int i = 1; i < tab.size(); i++)
    {
        if (tab[i] > wynik)
        {
            wynik = tab[i];
        }
    }
    return wynik;
}


bool czyNalezy(vector<int>tab,int element)
{
    for (int i = 0; i < tab.size(); i++)
    {
        if (element == tab[i])
        {
            return true;

        }
    }
    return false;
}

int main()
{
    vector<int>tab = { 8,5,4,7,3 };
    int element = 5;
    bool wynik = czyNalezy(tab,element);
    cout << wynik;
}
